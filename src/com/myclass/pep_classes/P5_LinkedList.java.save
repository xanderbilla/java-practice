package com.myclass.pep_classes;

class Node{
    public int val;
    public Node next;

    public Node(int data){
        this.val = data;
        this.next = null;
    }
}

class SLinkedList{
    public Node head;

    public SLinkedList(){
        this.head = null;
    }

    public void insertAtBeginning(int data){
        Node newNode = new Node(data);
        newNode.next = head;
        head = newNode;
    }

    public void insertAtEnd(int data){
        Node newNode = new Node(data);
        if(head.next == null){
            head.next = newNode;
            return;
        }

        Node temp = head;
        while(temp.next != null){
            temp = temp.next;
        }
        temp.next = newNode;
    }

   public void deleteAtFront(){
	Node temp = head;
	
	head = temp.next;



}

